{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"command": "./node_modules/.bin/astro dev",
			"name": "Development server",
			"request": "launch",
			"type": "node-terminal",
			"skipFiles": ["**/node_modules/**"]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:4321",
			"webRoot": "${workspaceFolder}/src",
			"sourceMapPathOverrides": {
				"vite:///src/*": "${webRoot}/*"
			},
			"skipFiles": ["**/node_modules/**"]
		},
		{
			"name": "SAM: HelloWorldFunction",
			"type": "node",
			"request": "attach",
			"address": "localhost",
			"port": 9229,
			"localRoot": "${workspaceFolder}/lambda/notifications/signup-processor",
			"remoteRoot": "/var/task",
			"skipFiles": ["<node_internals>/**"],
			"restart": true,
			"sourceMaps": true,
			"outFiles": [
				"${workspaceFolder}/lambda/notifications/signup-processor/dist/**/*.js"
			],
			"trace": true
		}
	],
	"compounds": [
		{
			"name": "Launch Dev Server and Chrome",
			"configurations": [
				"Development server",
				"Launch Chrome against localhost"
			]
		}
	]
}
