{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "aws-sam",
			"request": "direct-invoke",
			"name": "signup-processor:index.handler (nodejs22.x)",
			"invokeTarget": {
				"target": "code",
				"projectRoot": "${workspaceFolder}/backend/functions/signup-processor",
				"lambdaHandler": "index.handler"
			},
			"lambda": {
				"runtime": "nodejs22.x",
				"payload": {
					"path": "${workspaceFolder}/backend/events/signup-event-ALL-notifications-on.json"
				},
				"environmentVariables": {}
			}
		},
		{
			"type": "aws-sam",
			"request": "direct-invoke",
			"name": "nasa_photo_fetcher:index.handler (python3.13)",
			"invokeTarget": {
				"target": "code",
				"projectRoot": "${workspaceFolder}/backend/functions/nasa_photo_fetcher",
				"lambdaHandler": "index.handler"
			},
			"lambda": {
				"runtime": "python3.13",
				"payload": {},
				"environmentVariables": {
					"DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/postgres",
					"NASA_API_KEY": "DEMO_KEY",
					"S3_BUCKET": "nasa-images-bucket"
				}
			}
		},
		{
			"command": "npm run dev",
			"name": "Development server",
			"request": "launch",
			"type": "node-terminal",
			"skipFiles": ["**/node_modules/**"]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:4321",
			"webRoot": "${workspaceFolder}/frontend/src",
			"sourceMapPathOverrides": {
				"vite:///src/*": "${webRoot}/*"
			},
			"skipFiles": ["**/node_modules/**"]
		}
	],
	"compounds": [
		{
			"name": "Launch Dev Server and Chrome",
			"configurations": [
				"Development server",
				"Launch Chrome against localhost"
			]
		}
	]
}
