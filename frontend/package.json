{
	"name": "text-notifications-frontend",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npx astro dev --config config/astro.config.mjs",
		"start": "npx astro dev --config config/astro.config.mjs",
		"build": "npx astro check --config config/astro.config.mjs && npx astro build --config config/astro.config.mjs && node ../scripts/process-html.mjs",
		"preview": "npx astro preview --config config/astro.config.mjs",
		"astro": "npx astro --config config/astro.config.mjs",
		"generate-images": "node --loader ts-node/esm ../scripts/svg-to-png.ts",
		"clean": "rm -rf dist",
		"type-check": "npx tsc --noEmit -p src/tsconfig.json"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.3.1",
		"@astrojs/vue": "^5.0.13",
		"@heroicons/vue": "^2.2.0",
		"@tailwindcss/vite": "^4.1.6",
		"@vueuse/core": "^13.1.0",
		"astro": "^5.7.12",
		"fuse.js": "^7.1.0",
		"libphonenumber-js": "^1.12.8",
		"pg": "^8.15.6",
		"tailwindcss": "^4.1.6",
		"vue": "^3.5.13",
		"@text-notifications/shared": "*"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.149",
		"@types/node": "^22.15.17",
		"dotenv": "^16.5.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
